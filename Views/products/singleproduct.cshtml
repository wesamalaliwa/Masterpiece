                    <script>
    function increaseQuantity() {
        var result = document.getElementById('sst');
        var sst = parseInt(result.value);
        var maxQuantity = @ViewBag.Quantity;
        if (!isNaN(sst) && sst < maxQuantity) {
            result.value++;
        }
    }

    function decreaseQuantity() {
        var result = document.getElementById('sst');
        var sst = parseInt(result.value);
        if (!isNaN(sst) && sst > 1) {
            result.value--;
        }
    }
                    </script>

